package JavaArray;

public class Pyramid2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int row=5;
		for (int i=0;i>row;i++)
	{
			
			for (int j=i;j<=i;j++)
			{
								
				
                System.out.print("* ");
			}
				
          System.out.println();
	}

}
}