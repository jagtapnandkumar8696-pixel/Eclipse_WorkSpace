package StringAndArray;

public class Array_sort {

	public static void main(String[] args) {
		
		int a[]=new int[5];
		int temp;
		
		for(int i=0;i<a.length;i++)
		{
			a[i]=(int) (Math.random()*100);
			System.out.print(a[i]+" ");
		}
		System.out.println();
		for(int i=0;i<a.length;i++)
		{
			for(int j=1;j<a.length;j++)
			{
				if(a[i]<a[j])
				{
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		}
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}

	}

}
