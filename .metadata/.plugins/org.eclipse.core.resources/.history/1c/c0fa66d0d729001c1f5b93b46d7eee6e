package Loops;

public class L {

	public static void main(String[] args) {

    int a[]=new int[6];
    int i,j;
    int sum=0;
    
    for(i=0;i<6;i++)
    {
    	a[i]=(int)(Math.random()*10);
    	   System.out.print(a[i]);
    }
    sum=a[0];
    for(i=0;i<6;i++)
    {
    	if(sum>a[i])
    	{
    		sum=a[i];
    	}
    	
    }
    System.out.println("smallest is "+a[i]);
	}

}
