package job.ControlSructure;

public class IfelseLadder_Example1 {

	public static void main(String[] args) {
		
		int a=80;
		
		if (a>=35 && a>50)
		{
			System.out.println("Passing marks");
			
		}
				
		else if(a>=50 && a<65)
		{
			System.out.println("Second class");
		}
		else if(a>=65 && a<80)
		{
			System.out.println("First Class");
		}
		
		else if(a>=80)
		{
			System.out.println("First Class");
		}

	}

}
