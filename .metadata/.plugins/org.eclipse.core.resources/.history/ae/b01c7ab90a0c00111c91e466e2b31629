package Array;

public class MaxNumberinArray {

	public static void main(String[] args) {
		int a[]=new int[5];
		for(int i=0;i<a.length;i++)
		{
			a[i]= (int)(Math.random()*100);
			System.out.print(a[i]+ " ");
					
		}
		int max=a[0];
		for(int i=1;i<a.length;i++)
		{
			if(a[i]>max)
			{
				max=
			}
					
		}
		

	}

}
