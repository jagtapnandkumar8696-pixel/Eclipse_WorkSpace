package StringAndArray;

public class Two_D_Array {

	public static void main(String[] args) {
		
		int a[][]=new int [4][3];
		
		for (int i=0;i<a.length;i++)
		{
			for (int j=0;j<a[0].length;j++)
			{
				a[i][j]=(int) Math.random()*100;
				System.out.print(a[i][j]+ " ");
				
			}
			System.out.println();
		}
		

	}

}
