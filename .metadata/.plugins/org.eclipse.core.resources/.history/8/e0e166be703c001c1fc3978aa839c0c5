package constroctor;

import classNadAccessModifier.Class1;
class Constroctor {
  static int a;
	
}
public class Constroctor1 extends Constroctor{

	int a=12;
	int c;
	
	String s;
	static char ch;
	 Constroctor1()
	{display();
		a=101;
		s="Nandu";
		System.out.println("Test");
		
	}
 void display()
 {  // a=12;
	 System.out.println("A is "+a);
	
	 System.out.println("S is "+s);
 }
	public static void main(String[] args) {
		
		Constroctor1 c=new Constroctor1();
		Constroctor c1=new Constroctor();
		c1.a=13;
		Constroctor.a=45;
		
		c.a=8;
		c.display();
				
		
	}


}
